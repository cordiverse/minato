{
  "name": "minato",
  "version": "3.5.0",
  "description": "Type Driven Database Framework",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./cli": "./lib/cli.mjs",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "contributors": [
    "Shigma <shigma10826@gmail.com>",
    "Hieuzest <girkirin@hotmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/minato.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/minato/issues"
  },
  "homepage": "https://github.com/cordiverse/minato",
  "keywords": [
    "orm",
    "query",
    "database",
    "sql",
    "mysql",
    "sqlite",
    "mongo",
    "postgres",
    "cordis",
    "plugin"
  ],
  "cordis": {
    "ecosystem": {
      "pattern": [
        "@minatojs/driver-*",
        "@minatojs/plugin-*",
        "minato-plugin-*"
      ]
    },
    "service": {
      "implements": [
        "model"
      ]
    }
  },
  "devDependencies": {
    "cordis": "^3.18.0"
  },
  "peerDependencies": {
    "cordis": "^3.18.0"
  },
  "dependencies": {
    "cosmokit": "^1.6.2"
  }
}
